<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Vote">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/whoVote" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="156dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="171dp" android:layout_marginStart="171dp" android:layout_marginEnd="181dp"
            android:layout_marginRight="181dp" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="@string/success"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/good" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="270dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="156dp" android:layout_marginStart="156dp" android:layout_marginEnd="167dp"
            android:layout_marginRight="167dp" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="@string/fail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/bad" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="380dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="160dp" android:layout_marginStart="160dp" android:layout_marginEnd="163dp"
            android:layout_marginRight="163dp" app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>